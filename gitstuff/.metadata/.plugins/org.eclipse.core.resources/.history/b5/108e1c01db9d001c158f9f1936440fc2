package com.ajay;

import java.util.Scanner;

public class Factorial {

	public static void main(String[] args) {
	 Scanner sc=new Scanner(System.in);
	 System.out.println("input:");
	 int n=sc.nextInt();
	 int b=0;
	for(int i=0;i<=n;i++)
	 {
		 b=n*(n-1);
		 n--;
		 
	 }
		 
System.out.println(b);
	}

}
