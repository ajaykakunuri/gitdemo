package ajayintro;

import java.util.Scanner;

public class Oddnum {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("input:");
		int n=sc.nextInt();
		for(int i=0;i<=n;i++)
		{
			if(n%2!=0)
			
				System.out.println("odd number");
			
			else
				System.out.println("even");
		}

	}

}
